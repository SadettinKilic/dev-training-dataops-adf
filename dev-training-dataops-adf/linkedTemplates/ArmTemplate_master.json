{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-training-dataops-adf"
		},
		"datalakeLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeLS'"
		},
		"datalakeLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sttrainingdataops.dfs.core.windows.net/"
		},
		"gitLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"keyVaultLS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-training-dataops.vault.azure.net/"
		},
		"tr_dbt_gold_daily_12pm_properties_dbt_dataops_gold_daily_parameters_p_job_id": {
			"type": "string",
			"defaultValue": "469260498224433"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-training-dataops-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakeLS_accountKey": {
						"value": "[parameters('datalakeLS_accountKey')]"
					},
					"datalakeLS_properties_typeProperties_url": {
						"value": "[parameters('datalakeLS_properties_typeProperties_url')]"
					},
					"gitLS_properties_typeProperties_url": {
						"value": "[parameters('gitLS_properties_typeProperties_url')]"
					},
					"keyVaultLS_properties_typeProperties_baseUrl": {
						"value": "[parameters('keyVaultLS_properties_typeProperties_baseUrl')]"
					},
					"tr_dbt_gold_daily_12pm_properties_dbt_dataops_gold_daily_parameters_p_job_id": {
						"value": "[parameters('tr_dbt_gold_daily_12pm_properties_dbt_dataops_gold_daily_parameters_p_job_id')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}